{
  "expo": {
    "name": "listify",
    "slug": "listify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/listify.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/listify.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/listify.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/listify.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "API_URL": "http://192.168.0.13:4000/api"
    }
  }
}
